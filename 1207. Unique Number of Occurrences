1207. Unique Number of Occurrences
    def uniqueOccurrences(self, arr: List[int]) -> bool:
        
        #Dict to count the occurances of each element
        countdict = {}
        for i in arr:
            if i in countdict:
                countdict[i] +=1
            else:
                countdict[i] =1
                
                
        #List to store the occurances of each element        
        freqlist = []
        for i in countdict:
            freqlist.append(countdict[i])
            
            
        #If length of set and list is equal, then all the occurances are unique
        return len(set(freqlist)) == len(freqlist)
                
            
        
